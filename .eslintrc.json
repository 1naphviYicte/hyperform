{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-trailing-spaces": "off",
        "semi": "off",
        "object-shorthand": "off",
        "no-unused-vars": "warn",
        "no-useless-catch": "warn"
    }
}
